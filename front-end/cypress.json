{
    "projectId": "z5nx7c",
    "baseUrl": "http://localhost:8080/",
    "port": 9001,
    "ignoreTestFiles": [
        "**/__snapshots__/*",
        "**/__image_snapshots__/*"
    ],
    "browser": { "chromeWebSecurity": false },
    "env": {
        "cypress-plugin-snapshots": {
            "autoCleanUp": false,
            "autopassNewSnapshots": true,
            "diffLines": 3,
            "excludeFields": [],
            "ignoreExtraArrayItems": false,
            "ignoreExtraFields": false,
            "normalizeJson": true,
            "prettier": true,
            "imageConfig": {
                "createDiffImage": true,
                "resizeDevicePixelRatio": true,
                "threshold": 0.01,
                "thresholdType": "percent"
            },
            "screenshotConfig": {
                "blackout": [],
                "capture": "fullPage",
                "clip": null,
                "disableTimersAndAnimations": true,
                "log": false,
                "scale": false,
                "timeout": 30000
            }
        }
    }
}
